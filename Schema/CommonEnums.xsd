    <xsd:simpleType name="AssemblyRelationshipEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Permanent"/>
            <xsd:enumeration value="Transient"/>
            <xsd:enumeration value="Other"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!--     -->
